<div class="row">
  <div class="col-md-6">
    <h3>House Committees</h3>

    <ul>
        {{#each committee in model}}
            <li>
              {{#link-to 'subcommittees' committee.committee_id}}{{committee.name}}{{/link-to}}
            </li>
        {{/each}}
    </ul>
  </div>

  <div class="col-md-6">
    {{outlet}}
  </div>
</div>
